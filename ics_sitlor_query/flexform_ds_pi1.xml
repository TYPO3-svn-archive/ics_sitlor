<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<meta>
       <langDisable>1</langDisable>
	</meta>
	<sheets>
		<main>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:general</sheetTitle>
				</TCEforms>
				<el>
					<template>
						<TCEforms>
							<label>LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:template</label>
						   <config>
								<type>input</type>
								<eval>trim</eval>
								<default></default>
								<wizards type="array">
									<_PADDING>2</_PADDING>
									<link type="array">
										<type>popup</type>
										<title>Link</title>
										<icon>link_popup.gif</icon>
										<script>browse_links.php?what_to_display=wizard&amp;act=file</script>
										<params type="array">
											<blindLinkOptions>page,url,mail,spec</blindLinkOptions>
											<allowedExtensions>htm,html,tmpl,tpl</allowedExtensions>
										</params>
										<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
									</link>
								</wizards>
							</config>
						</TCEforms>
					</template>
					<url>
						<TCEforms>
							<label>LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:url</label>
							<config>
								<type>input</type>
								<size>50</size>
							</config>
						</TCEforms>
					</url>
					<login>
						<TCEforms>
							<label>LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:login</label>
							<config>
								<type>input</type>
								<size>30</size>
							</config>
						</TCEforms>
					</login>
					<password>
						<TCEforms>
							<label>LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:password</label>
							<config>
								<type>input</type>
								<size>30</size>
							</config>
						</TCEforms>
					</password>
					<what_to_display>
						<TCEforms>
							<label>LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:what_to_display</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array"></numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:search</numIndex>
										<numIndex index="1">SEARCH</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:list</numIndex>
										<numIndex index="1">LIST</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:single</numIndex>
										<numIndex index="1">SINGLE</numIndex>
									</numIndex>
								</items>
								<size>4</size>
								<minitems>0</minitems>
								<maxitems>3</maxitems>
							</config>
						</TCEforms>
					</what_to_display>
					<size>
						<TCEforms>
							<label>LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:size</label>
							<config>
								<type>input</type>
								<eval>num</eval>
								<size>5</size>
							</config>
						</TCEforms>
					</size>
				</el>
			</ROOT>
		</main>
		<paramSelect>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:paramSelect</sheetTitle>
				</TCEforms>
				<el>
					<dataGroup>
						<TCEforms>
							<label>LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:dataGroup</label>
							<onChange>reload</onChange>
							<config type="array">
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array"></numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:dataGroup_accomodation</numIndex>
										<numIndex index="1">ACCOMODATION</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:dataGroup_restaurant</numIndex>
										<numIndex index="1">RESTAURANT</numIndex>
