									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:dataGroup_event</numIndex>
										<numIndex index="1">EVENT</numIndex>
									</numIndex>
								</items>
								<size>1</size>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</dataGroup>
					<!-- ###SUBDATAGROUP### -->
					<OTNancySubscriber>
						<TCEforms>
							<label>LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:OTNancySubscriber</label>
							<config>
								<type>input</type>
								<size>30</size>
							</config>
						</TCEforms>
					</OTNancySubscriber>
					<noDate>
						<TCEforms>
							<label>LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:noDate</label>
							<config>
								<type>input</type>
								<size>2</size>
							</config>
						</TCEforms>
					</noDate>
				</el>
			</ROOT>
		</paramSelect>
		<paramSorting>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:paramSorting</sheetTitle>
				</TCEforms>
				<el>
					<sortName>
						<TCEforms>
							<label>LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:sortName</label>
							<config type="array">
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array"></numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:sortName_random</numIndex>
										<numIndex index="1">RANDOM</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:sortName_alpha</numIndex>
										<numIndex index="1">ALPHA</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:sortName_price</numIndex>
										<numIndex index="1">PRICE</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:sortName_rating</numIndex>
										<numIndex index="1">RATING</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:sortName_date</numIndex>
										<numIndex index="1">DATE</numIndex>
									</numIndex>
								</items>
								<size>1</size>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</sortName>
					<sortOrder>
						<TCEforms>
							<label>LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:sortOrder</label>
							<config type="array">
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array"></numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:sortOrder_asc</numIndex>
										<numIndex index="1">asc</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:ics_sitlor_query/locallang_flexform_pi1.xml:sortOrder_desc</numIndex>
										<numIndex index="1">desc</numIndex>
									</numIndex>
								</items>
								<size>1</size>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</sortOrder>
				</el>
			</ROOT>
		</paramSorting>
	</sheets>
</T3DataStructure>